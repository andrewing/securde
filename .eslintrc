{
  "extends": ["airbnb", "prettier"],
   "parserOptions": {
     "ecmaVersion": 2018,
     "sourceType": "module"
   },
   "env": {
     "browser": true,
     "node": true,
     "es6": true,
   },
   "plugins": [],
   "rules": {
     "no-unused-vars": [
       "warn",
       {
         "vars": "local",
         "args": "none"
       }
     ],
   }
 }