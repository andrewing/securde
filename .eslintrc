{
  "extends": ["airbnb", "prettier"],
   "parserOptions": {
     "ecmaVersion": 2018,
     "sourceType": "module"
   },
   "env": {
     "browser": true,
     "node": true,
     "es6": true,
   },
   "plugins": [],
   "rules": {
     "no-unused-vars": [
       "off",
       {
         "vars": "local",
         "args": "none"
       }
     ],
     "react/jsx-filename-extension": "off",
     "no-use-before-define": "off",
     "no-param-reassign": "off",
     "import/prefer-default-export": "off",
     "no-underscore-dangle": "off",
   }
 }