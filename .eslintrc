{
  "extends": ["airbnb", "prettier"],
   "parserOptions": {
     "ecmaVersion": 2018,
     "sourceType": "module"
   },
   "env": {
     "browser": true,
     "node": true,
     "es6": true,
   },
   "plugins": [],
   "rules": {
     "no-unused-vars": [
       "warn",
       {
         "vars": "local",
         "args": "none"
       }
     ],
     "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
     "no-use-before-define": ["warn", { "functions": true, "classes": true }]
   }
 }